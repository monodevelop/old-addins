<Addin id = "Mono.Debugging.Mdb"
       name	     = "MDB support for Mono.Debugging"
       author	 = "Ankit Jain, Lluis Sanchez"
       url       = "http://www.mono-project.com"
       description = "Managed Debugging Engine support for MDB"
       copyright   = "MIT X11"
       version   = "0.1">

	<Dependencies>
		<Addin id="Mono.Debugging" version="0.1"/>
	</Dependencies>
	
	<Extension path="/Mono/Debugging/DebuggerFactories">
		<DebuggerFactory type="Mono.Debugging.Backend.Mdb.MonoDebuggerSessionFactory" />
	</Extension>

</Addin>
