<Addin id = "MonoDevelop.Debugger.Soft"
       name	     = "Support for Mono Soft Debugger"
       author	 = "Lluis Sanchez"
       url       = "http://www.mono-project.com"
       description = "GNU Debugger support for Mono.Debugging"
       copyright   = "MIT X11"
       category = "Debugging"
       version   = "2.1.0">

	<Dependencies>
		<Addin id="MonoDevelop.Core" version="2.1.0"/>
		<Addin id="MonoDevelop.Debugger" version="2.1.0"/>
	</Dependencies>
	
	<Extension path="/MonoDevelop/Debugging/DebuggerFactories">
		<DebuggerFactory type="MonoDevelop.Debugger.Soft.SoftDebuggerEngine" />
	</Extension>

</Addin>
